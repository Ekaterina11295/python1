q = int(input("Введите количество элементов в списке: "))
list = []
i = 0
el = 0
while i < q:
    list.append(input("Введите следующее значение в списке: "))
    i += 1
for element in range(int(len(list)/2)):
        list[el], list[el + 1] = list [el + 1], list[el]
        el += 2
print(list)