my_list = [7, 5, 3, 3, 2]
print(my_list)
rate = int(input("Введите элемент рейтинга: "))
for el in range(len(my_list)):
    if my_list[el] == rate:
        my_list.insert(el + 1, rate)
        break
    elif my_list[0] < rate:
        my_list.insert(0, rate)
        break
    elif my_list[-1] > rate:
         my_list.append(rate)
         break
    elif my_list[el] > rate and my_list[el + 1] < rate:
         my_list.insert(el + 1, rate)
         break
print(my_list)